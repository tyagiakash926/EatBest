extends base.pug
block header
 include homepageheader.pug
block content
  .main-container
   .glass-container
    .dashboard-section
      .dashboard-section-div-1
        #homeurl.dashboard-section-div-1-button
          .dashboard-section-div-1-button-icon
            i.fas.fa-archway
          .dashboard-section-div-1-button-url
            | Home
        #reviewurl.dashboard-section-div-1-button
          .dashboard-section-div-1-button-icon
            i.fas.fa-comments
          .dashboard-section-div-1-button-url
            | review
        #detailurl.dashboard-section-div-1-button
          .dashboard-section-div-1-button-icon
            i.fas.fa-info-circle
          .dashboard-section-div-1-button-url
            | Details
        #loginurl.dashboard-section-div-1-button
          .dashboard-section-div-1-button-icon
            i.fas.fa-sign-in-alt
          .dashboard-section-div-1-button-url
            | Login
      .dashboard-section-div-2
        .dashboard-section-div-2-running-svg
          img(src='/images/undraw_street_food_hm5i 1 (1).svg')
        .dashboard-section-div-2-heading
          h1 Faster Delivery!
          h2 Learn more
    .plans-section
      .plans-section-1
        .plans-section-1-1
          .plans-section-1-heading
            h1
              | Welcome to 
              br
              span(id="changingtext") Eatland
              img(src='data:image/svg+xml;base64,')
          .plans-section-1-cart
            img(src='data:image/svg+xml;base64,')
        .plans-section-1-2
          .plans-section-1-2-heading-planss
            .plans-section-1-2-heading-plans.active
              img(src='data:image/svg+xml;base64,')
              p All
            .plans-section-1-2-heading-plans
              img(src='data:image/svg+xml;base64,')
              p Burger
            .plans-section-1-2-heading-plans
              img(src='data:image/svg+xml;base64,')
              p Pizza
            .plans-section-1-2-heading-plans
              img(src='data:image/svg+xml;base64,')
              p salad
            .plans-section-1-2-heading-plans
              img(src='data:image/svg+xml;base64,')
              p Desert
            .plans-section-1-2-heading-plans
              img(src='data:image/svg+xml;base64,')
              p Drinks
          .plans-section-1-2-discount
            img#burger(src='data:image/svg+xml;base64,')
            img#burger-leaf(src='data:image/svg+xml;base64,')
            img#burger-bean(src='data:image/svg+xml;base64,')
            h1 -10% off first user
      .plans-section-2
        .all-items-filter
          h1
            | All items 
            img(src='data:image/svg+xml;base64,')
        .plans-section-2-plans
          each plan in plans
            include choteplan.pug
block scripts
 script(src="https://js.stripe.com/v3/")
 script(src="js/planpage.js")
 script(src="js/buyplan.js")
